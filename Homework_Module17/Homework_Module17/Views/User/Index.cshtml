@model List<UserViewModel>

@{
    ViewData["Title"] = "User";
}

<ul>
    @foreach(var user in Model){
        @: <li>@($"Name: {user.Name}, Sex: {user.Sex}, Country: {user.Country}, IsMarried: {user.IsMarried}")</li>
    }
</ul>

@Html.ActionLink("AddNewUser", "AddUser")
@Html.ActionLink("GetFirstUser", "GetUser")
@Html.ActionLink("DeleteUser", "DeleteUser")