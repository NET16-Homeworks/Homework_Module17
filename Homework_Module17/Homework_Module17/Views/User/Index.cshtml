@model List<UserViewModel>;

<h1>Users: </h1>

<ul>
    @if (@Model.Count == 0)
    {
        <h2>No users is here...... NEEEEEEEEEEEED MORE USERS</h2>
    }
    else
    {
        @foreach (UserViewModel user in @Model)
        {
            <li>
                @await Html.PartialAsync("_User", user)
            </li>
        }
    }

</ul>

@Html.ActionLink("Add New User", "AddUser")
@Html.ActionLink("GetFirstUser", "GetUser")
@Html.ActionLink("DeleteUser", "DeleteUser")